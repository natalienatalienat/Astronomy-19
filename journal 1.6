import numpy as np

def sin_function(x):
    return np.sin(x)

def cos_function(x):
    return np.cos(x)

x_values = np.linspace(0, 2 * np.pi, 1000)
sin_values = sin_function(x_values)
cos_values = cos_function(x_values)
table = np.column_stack((x_values, sin_values, cos_values))

print(f"{'x'} {'sin(x)'} {'cos(x)'}")
for i in range(10):
    x, sin_x, cos_x = table[i]
    print(f"{x} {sin_x} {cos_x}")
